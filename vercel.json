{
  "version": 2,
  "builds": [
    {
      "src": "api/index.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    { "src": "/api/babynames", "dest": "/api/index.go" },
    { "src": "/api/babynames/year", "dest": "/api/index.go" },
    { "src": "/api/babynames/gender", "dest": "/api/index.go" },
    { "src": "/api/babynames/ethnicity", "dest": "/api/index.go" },
    { "src": "/swagger/(.*)", "dest": "/api/index.go" }
  ]
}
