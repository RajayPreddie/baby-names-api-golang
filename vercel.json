{
  "version": 2,
  "builds": [
    {
      "src": "api/*.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    { "src": "/api/babynames", "dest": "/api/get_baby_names.go" },
    { "src": "/api/babynames/year", "dest": "/api/get_baby_names_by_year.go" },
    { "src": "/api/babynames/gender", "dest": "/api/get_baby_names_by_gender.go" },
    { "src": "/api/babynames/ethnicity", "dest": "/api/get_baby_names_by_ethnicity.go" },
    { "src": "/", "dest": "/api/index.go" }
  ]
}
